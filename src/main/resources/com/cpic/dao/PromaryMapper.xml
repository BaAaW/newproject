<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cpic.dao.PromaryMapper">
<!--根据proName查询proID-->
    <select id="selectProid" resultType="Integer">
